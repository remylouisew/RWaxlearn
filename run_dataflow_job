--region=us-west1 \
--temp_location=gs://dataflow-test-bucket0/rwtest/temp/ \
--staging_location=gs://dataflow-test-bucket0/rwtest/staging \
   --disk_size_gb=50 \
   --runner=DataflowRunner \
   --worker_machine_type=n1-standard-16 \
